{
  "name": "openim-web",
  "version": "1.0.2",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/openim-web.umd.js",
  "module": "./dist/openim-web.es.js",
  "exports": {
    ".": {
      "import": "./dist/openim-web.es.js",
      "require": "./dist/openim-web.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "build:lib": "vue-tsc && vite build --config vite.lib.config.ts",
    "prepublishOnly": "npm run build",
    "release": "npm version patch --no-git-tag-version && npm publish --access public"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.3.0",
    "vue": "^3.5.13",
    "vue-i18n": "12.0.0-alpha.2",
    "vue-router": "^4.5.0",
    "vxe-pc-ui": "4.6.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.0",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "keywords": [
    "vue3",
    "ui",
    "im",
    "chat",
    "components"
  ],
  "author": "Qiutianxiaxue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qiutianxiaxue/openim-web.git"
  },
  "bugs": {
    "url": "https://github.com/Qiutianxiaxue/openim-web/issues"
  },
  "homepage": "https://github.com/Qiutianxiaxue/openim-web#readme",
  "license": "MIT"
}
